{
  "name": "build-ecommerce-website-like-amazon",
  "type": "module",
  "version": "1.0.0",
  "description": "e-com",
  "main": "server.js",
  "scripts": {
    "start": "nodemon --watch backend --exec babel-node backend/server.js",
    "build": "del -rf dist && babel backend -d dist",
    "heroku-postbuild": "npm run build && cd frontend && npm install && npm run build",
    "server": "npm start",
    "client": "cd frontend && npm  start",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajeetsing/Build-Ecommerce-Website-Like-Amazon.git"
  },
  "author": "ajeet singh thakur",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^7.2.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-handler": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react-hooks": "^2.5.1",
    "nodemon": "^2.0.2"
  },
  "bugs": {
    "url": "https://github.com/ajeetsing/Build-Ecommerce-Website-Like-Amazon/issues"
  },
  "homepage": "https://github.com/ajeetsing/Build-Ecommerce-Website-Like-Amazon#readme"
}
